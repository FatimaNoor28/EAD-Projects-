<section id="appointment" class="appointment section-bg">
    <div class="container">

        <div class="section-title">
            <h2>Assign Room to Patient</h2>
        </div>

        <form action="Admin.html">
            <div class="row form-row">
                <div class="col-md-4 form-group mt-3 mt-md-0 ">
                    <label class="label">Enter Patient ID</label>   <input type="text" name="Id" class="form-control" />
                </div>
                <div class="col-md-4 form-group mt-3 mt-md-0 ">
                    <label class="label">Enter Room Number</label>   <input type="text" name="RoomNo" class="form-control" />
                </div>
            </div>
            <div class="bttn text-center">
                <input type="submit" value="Assign Room" />
            </div>
        </form>
    </div>
</section>
