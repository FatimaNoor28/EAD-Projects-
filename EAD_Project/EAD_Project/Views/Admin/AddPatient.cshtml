<!doctype html>
<html lang="en">

    <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
            integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
            crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
            integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
            crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
            integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
            crossorigin="anonymous"></script>
    <link rel="stylesheet" href="css/our.css">

    </head>

    <body>

    @*<link rel="stylesheet" href="lib\bootstrap\dist\css\our.css">*@

    <section id="appointment" class="appointment section-bg">
        <div class="container">

            <div class="section-title">
                <h2>Add Patient</h2>
            </div>

            <form action="Admin">
                <div class="row form-row">
                    <div class="col-md-4 form-group mt-3 mt-md-0 ">
                        <label class="label">Enter Patient ID</label>   <input type="text" name="Id" class="form-control" />
                    </div>
                    <div class="col-md-4 form-group mt-3 mt-md-0 form-col">
                        <label class="label">Enter Name</label>   <input type="text" name="Name" class="form-control" />
                    </div>
                </div>
                <div class="row form-row">
                    <div class="col-md-4 form-group mt-3 mt-md-0">
                        <label class="label">Enter CNIC</label>   <input type="text" name="CNIC" class="form-control" />
                    </div>
                    <div class="col-md-4 form-group mt-3 mt-md-0 form-col">
                        <label class="label">Enter Phone Number</label>   <input type="text" name="PhoneNum" class="form-control" />
                    </div>
                </div>
                <div class="row form-row">
                    <div class="col-md-4 form-group mt-3 mt-md-0">
                        <label class="label">Assigned Doctor</label>   <input type="text" name="Doctor" class="form-control" />
                    </div>
                    <div class="col-md-4 form-group mt-3 mt-md-0 form-col">
                        <label class="label">Enter Patient Room</label>   <input type="text" name="RoomNo" class="form-control" />
                    </div>
                </div>

                <div class="text-center bttn">
                    <input type="submit" value="Add Patient" />
                </div>
            </form>

        </div>
    </section>


</body>
</html>